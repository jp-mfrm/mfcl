{
  "name": "mfcl",
  "version": "0.1.1",
  "private": false,
  "description": "React Component Library for UI consistency between repositories",
  "main": "./dist/index.js",
  "typings": "./dist/lib/index.d.ts",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "prebuild": "npm run test:coverage",
    "prepublishOnly": "npm run build",
    "build": "rollup -c && node ./scripts/buildDist",
    "build:web": "docz build",
    "serve": "docz serve",
    "dev": "yarn start",
    "lint": "eslint --fix ./src",
    "plop": "plop",
    "start": "docz dev -o",
    "stylelint": "stylelint ./src",
    "test": "cross-env CI=1 react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "watch": "rollup -c -w"
  },
  "dependencies": {},
  "peerDependencies": {
    "clsx": "^1.1.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-transition-group": "^4.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@testing-library/react-hooks": "^3.4.1",
    "@testing-library/user-event": "^12.1.1",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-transition-group": "^4.4.0",
    "clsx": "^1.1.1",
    "colors": "^1.3.3",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "docz": "^2.3.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "node-sass": "^4.13.0",
    "plop": "^2.5.3",
    "prettier": "^2.0.5",
    "react": "^16.11.0",
    "react-component-component": "^1.2.1",
    "react-dom": "^16.11.0",
    "react-scripts": "^3.4.3",
    "react-test-renderer": "^16.13.1",
    "react-transition-group": "^4.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.5",
    "rollup-plugin-typescript2": "^0.27.2",
    "sass-loader": "^9.0.3",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/documentation/**",
      "!src/lib/index.ts",
      "!src/lib/declaration.d.ts",
      "!src/react-app-env.d.ts",
      "!src/setupTests.js"
    ]
  },
  "files": [
    "dist"
  ]
}
